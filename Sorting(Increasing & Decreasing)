#include<bits/stdc++.h>
 using namespace std;
bool cmp(int a,int b){
    cout<<a<<" "<<b<<endl;
    if(b>a) return 0;
    else return 1;
}

 int main(){
     int n;
     cin>>n;
     vector<int>arr(n);
     for(int i=0;i<n;i++){
         cin>>arr[i];
     }
      // Sort(arr.begin(),arr.end(),greater<int>())
     sort(arr.begin(),arr.end(),cmp);
     for(const int i:arr){
         cout<<i<<" ";
     }
 }
